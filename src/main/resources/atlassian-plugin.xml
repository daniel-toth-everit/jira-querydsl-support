<atlassian-plugin key="org.everit.jira.jira-querydsl-support" name="Querydsl Support for JIRA Plugin"
  plugins-version="2">
  <plugin-info>
    <version>${project.version}</version>
    <description>This plugin provides the Querydsl framework for JIRA. Plugins by Everit Kft. are
      depending on this plugin. Do not disable or uninstall it unless you are uninstalling Everit
      products because some plugins or features will not work.
    </description>
    <vendor name="EverIT Kft." url="https://www.everit.biz" />
    <param name="plugin-icon">images/pluginIcon.png</param>
    <param name="vendor-icon">images/vendorIcon.png</param>
    <param name="vendor-logo">images/vendorLogo.png</param>
  </plugin-info>

  
</atlassian-plugin>
